int main()
{
    int a,b,c;
    cout<<"enter first number (a): ";cin>>a;
    cout<<"enter second number (b): ";cin>>b;
    cout<<"enter third number (c): ";cin>>c;
    if(a>b&&a>c){
        if(b>c){
            cout<<"a is the greatest number and b is mid and c is the smallest number";
        }
        else{
            cout<<"a is gratest number and c is in mid and b is the smallest number";
        }
    }
    else if(b>>a&&b>>c){
        if(a>c){
            cout<<"b is the greatest number and a is in the mid and c is the smallest ";
        }else{
            cout<<"b is the greatest number and c is in the mid and a is the smallest";
        }
    }
    else if(c>b&&c>a){
        if(b>a){
            cout<<"c is the gratest number and b is in the mid and a is the smallest ";
        }else{
            cout<<"c is the greatest number and c is in the mid and b is the smallest num";
        }
    }
    
    

    return 0;
}
