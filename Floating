#include <iostream>
using namespace std;
 
int main()
{
   // A and B have same values but opposite signs
   int A = -500000000;
   int B =  500000000;
 
   int C = 1;
 
   cout << " A + (B + C) is equal to " << A + (B + C) << endl;
   cout << "(A + B) + C is equal to " << (A + B) + C << endl;
 
   return 0;
}
