#include <iostream>
using namespace std;

// Function with default height 'h' argument
double calcArea(double l, double h = 10.0) {
    return l * h;
}

int main() {
  
      // Uses default height
    cout << calcArea(5) << endl;
  
      // Uses custom height
    cout << calcArea(5, 7);
    return 0;
}
